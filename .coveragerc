[run]

source =
    archimedic
    api
    tests

omit =
    demo.py

parallel = True
context = '${CONTEXT}'

[report]
precision = 2
exclude_lines =
    pragma: no cover
    pragma: nocover
    pragma no cover
    noqa
    raise NotImplementedError
    raise NotImplemented
    @overload
    if TYPE_CHECKING:
